<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>NQuery.UI</id>
    <version>0.0.0</version>
    <authors>Immo Landwerth</authors>
    <owners>Immo Landwerth</owners>
    <licenseUrl>http://nquery.codeplex.com/license</licenseUrl>
    <projectUrl>http://nquery.codeplex.com</projectUrl>
