    <summary>
      This package provides additional UI controls for query authoring.
    </summary>
    <description>
      This package provides additional UI controls for query authoring including a show plan control to visualize
      the execution plan similar to Microsoft SQL Server Management Studio as well as a browser control to explore
      the scope of a query or expression.
    </description>
    <tags>SQL Query IEnumerable</tags>
    <dependencies>
      <dependency id="NQuery" />
    </dependencies>
  </metadata>
  <files>
    <file src="Bin\NQuery.UI.dll" target="lib\net20" /> 
    <file src="Bin\NQuery.UI.pdb" target="lib\net20" /> 
    <file src="Src\**\*.cs" target="src" />
  </files>
</package>
